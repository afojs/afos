    "@types/jest": "^29.2.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "ts-jest": "^29.0.3",
    "turbo": "^1.6.3"
  },
  "packageManager": "pnpm@7.8.0",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
